(function($) {
	$ = $.noConflict()

/*Clear form*/
function InputClear() {
	var query = $("#queryClear");
	var q = $("#formQuery").val();
	var regexp = /\w/;
	if ((!$.trim(q).length || !regexp.test(q)) && q > 1) {
	} else {
		if (q) {
			query.show();
			query.click(function() {
				$("#formQuery").attr('value', '').focus();
				query.css({display: "none"}); 
			});
		} 
		else {
			query.hide();
		}
	}
}

$(function() {
	$("#formQuery").bind("keyup", InputClear)
});
	
/* fade In "Введите запрос" */
$(document).ready(function() {
	var formQuery = $("#formQuery");
	var button = $('.go-form__beaty-button_button');
	formQuery.focus();
	var space=/^\s+$/;
	
	$('#Msearch').bind('submit', function(e) { 
		if($('#formQuery').val() == '' || space.test($('#formQuery').val())){
			e.preventDefault(); 	
			if (button.attr('disabled') != 'disabled') {
				button.attr("disabled","disabled");
				$('#fadeIn_txt').stop(true, true).text('Введите запрос').fadeIn(300).fadeOut(1500, function() {
					button.removeAttr("disabled");
				});
				formQuery.focus();
			}
		}
	});
});

