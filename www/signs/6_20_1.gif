var STP = STP || {};

jQuery(function( $ ) {
	var $form = $( document.forms.Msearch ),
		field = $( "#formQuery" ),
		suggestsWrapper = $( "#go-suggests" ),
		suggestsInnr = $( "#go-suggests-items" ),
		suggestsLoading = $( "#go-suggests-loading" ),
		suggestsItemTmpl = $( "#go-suggests-tmpl" );
	
	var suggests = new SG({
			enabled: false,
			field: field[ 0 ],
			cont: suggestsWrapper[ 0 ],
			list: suggestsInnr[ 0 ],
			switcher: "go-suggests_open",
			autoSubmit: true,
			preview: true,
			hover: "mrp-item-select",
			max: 7,
			url: {
				scheme: "",
				authority: "suggests.go.mail.ru",
				path: "sg_u",
				query: {
					q: "{query}"
				}
			},
			reqTimeout: 3000,
			scriptCharset: "utf-8",
			
			// FIXME Dirty hack.
			// Заменяем органические навиги, на навиги из отдельного поля
			dataFilter: function( data ) {
				if( data && data.items ) {
					var dataItems = data.items,
						dataItem,
						i,
						l,
						dataSitesLength = data.sites && data.sites.length || 0,
						dataMigrationSites = [];
					
					for( i = 0, l = dataSitesLength; i < l; i++ ) {
						dataItem = data.sites[ i ];
						if( dataItem.type === "site" ) {
							dataMigrationSites.push( dataItem );
						}
					}
					
					i = dataItems.length;
					while( i-- ) {
						dataItem = dataItems[ i ];
						if( dataItem.type === "site" ) {
							dataItems.splice( i, 1 );
						}
					}
					
					if( dataMigrationSites.length ) {
						dataItems.unshift.apply( dataItems, dataMigrationSites );
					}
				}
				return data;
			},
			item: suggestsItemTmpl.get( 0 ),
			result: function( itemData ) {
				return itemData.text;
			},
			
			onRenderEnd: function() {
				if( $.browser.msie && $.browser.version < 7 ) {
					suggestsWrapper.hide().show();
				}
			},
			onStartRequest: function() {
				suggestsLoading.show();
			},
			onStopRequest: function() {
				suggestsLoading.hide();
			}
		});
	
	
	suggests.opts({
		select: function( data ) {
			var itemData = data.itemData,
				result = data.result,
				value = data.value;
			
			if( itemData.type === "site" ) {
				var form = $( "<form>" ).attr({
					action: "//go.mail.ru/search",
					method: "get"
				});
				
				$.each({
					q: itemData.link,
					sg: data.value,
					ce: 1,
					sgsig: itemData.sig
				}, function( name, value ) {
					if( value != null ) {
						$( "<input>", {
							type: "hidden",
							name: name,
							value: value
						}).appendTo( form );
					}
				});
				
				// redirect
				form.appendTo( document.body )
					.submit();
				setTimeout(function() {
					form.remove();
				}, 50 );
				
				// prevent submit
				return false;
			}
				
			var moreData = {
				us: value.length,
				ussp: itemData.spcAuto || null,
				usln: data.selected + 1
			};
			
			var isMusSugg = STP.sgmus && itemData.mus;
			if( isMusSugg ) {
				moreData.usmus = 1;
				moreData.rch = "l";
				$form.find("input[name=rch]").remove();
			}
			
			var $inputs = $();
			$.each( moreData, function( name, value ) {
				if( value != null ) {
					$inputs = $inputs.add( $( "<input>", {
						type: "hidden",
						name: name,
						value: value
					}));
				}
			});
			
			$form.append( $inputs );
			
			setTimeout(function() {
				$inputs.remove();
			}, 10);
		}
	});
	
	if( $.browser.msie && $.browser.version < 7 ) {
		suggests.on( "closeEnd", function() {
			suggestsWrapper.css( "display", "none" ).css( "display", "" );
		});
	}
	
	// Включаем саджест
	suggests.enable();
});

})(jQuery)
	</script>

